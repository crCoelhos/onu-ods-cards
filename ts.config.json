{
    "compilerOptions": {
        "target": "es6",
        "module": "commonjs",
        "jsx": "react",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "baseUrl": "./",
        "paths": {
            "un-ods-icons": [
                "node_modules/un-ods-icons/dist/index"
            ]
        },
        "include": [
            "src/**/*",
            "node_modules/un-ods-icons/**/*"
        ]
    }
}